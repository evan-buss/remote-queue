{
  "name": "electron-quick-start-typescript",
  "version": "1.0.0",
  "description": "A minimal Electron application written with Typescript",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "npm run build && electron ./dist/main.js",
    "native": "npm rebuild --runtime=electron --target=4.2.9 --disturl=https://atom.io/download/atom-shell --abi=68"
  },
  "repository": "https://github.com/electron/electron-quick-start-typescript",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "dependencies": {
    "robotjs": "github:sidharth0094/robotjs#master",
    "express": "^4.17.1",
    "ws": "^7.1.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^12.7.2",
    "@types/ws": "^6.0.2",
    "electron": "^4.2.9",
    "electron-rebuild": "^1.8.6",
    "typescript": "^3.5.3"
  }
}
